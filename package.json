{
  "name": "@yanwsh/mack",
  "repository": {
    "type": "git",
    "url": "https://github.com/yanwsh/mack.git"
  },
  "version": "1.0.0",
  "description": "Converts Markdown to Slack BlockKit Blocks",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "lint": "gts lint",
    "clean": "rm -rf lib build",
    "compile": "tsup",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "release": "release-it"
  },
  "keywords": [
    "markdown",
    "parser",
    "gfm",
    "slack"
  ],
  "author": "Matthew Herod <matthew.herod@gmail.com>",
  "contributors": [
    "Instantish (original author)",
    "Fabric Team (tryfabric/mack)"
  ],
  "license": "MIT",
  "dependencies": {
    "@slack/types": "^2.1.0",
    "fast-xml-parser": "^4.0.6",
    "marked": "^4.0.12"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/marked": "^4.0.2",
    "@types/node": "^15.12.4",
    "gts": "^3.1.0",
    "jest": "^30.2.0",
    "release-it": "^14.11.6",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
